{
    "name": "project/php-framework",
    "type": "project",
    "description": "Simple PHP MVC Framework",
    "keywords": [
        "framework",
        "PHP",
        "php-framework"
    ],
    "license": "proprietary",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}